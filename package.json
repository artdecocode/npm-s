{
  "name": "@artdeco/npm-s",
  "version": "0.0.0-pre",
  "description": "Run NPM commands in series.",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "dev": "node src/bin",
    "compile": "depack src/bin/npm-s -o compile/bin/npm-s.js -a -c -S --externs types/externs.js",
    "lib": "depack src/depack.js -o compile/npm-s.js -a -c --source_map_include_content",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "doc -o README.md -n _npmS -a -d",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js types/index.js -u -t types/index.xml",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "externs": "typal types/externs.js",
    "args": "argufy -o src/bin/get-args.js"
  },
  "bin": {
    "npm-s": "compile/bin/npm-s.js",
    "npm-s-dev": "src/bin/index.js"
  },
  "files": [
    "src",
    "compile",
    "types/externs.js",
    "types/index.js",
    "src/bin/index.js",
    "typedefs.json"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecocode/npm-s.git"
  },
  "keywords": [
    "npm",
    "scripts"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/artdecocode/npm-s/issues"
  },
  "homepage": "https://www.artd.eco",
  "devDependencies": {
    "alamode": "^3.3.0",
    "argufy": "^1.7.5",
    "depack": "^1.0.1",
    "documentary": "^1.35.9",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "indicatrix": "^1.4.0",
    "usually": "^1.2.0",
    "yarn-s": "1.2.1",
    "zoroaster": "^4.1.2"
  }
}